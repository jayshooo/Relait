{
    "name": "relait",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ios": "sh ./iOS.sh",
        "ios-6s-serve": "tsc && npm run reset-metro && react-native run-ios --simulator 'iPhone 6s'",
        "ios-max-serve": "tsc && npm run reset-metro && react-native run-ios --simulator 'iPhone 11 Pro Max'",
        "start": "react-native start",
        "reset-metro": "rm -rf $TMPDIR/metro-* && watchman watch-del-all",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.10.3",
        "@react-native-community/masked-view": "^0.1.10",
        "@react-native-community/netinfo": "^5.8.0",
        "@react-native-seoul/kakao-login": "^2.4.5",
        "@react-navigation/native": "^5.3.2",
        "@react-navigation/stack": "^5.3.5",
        "@types/react-redux": "^7.1.8",
        "react": "16.11.0",
        "react-native": "0.62.2",
        "react-native-gesture-handler": "^1.6.1",
        "react-native-permissions": "^2.1.5",
        "react-native-reanimated": "^1.8.0",
        "react-native-restart": "0.0.17",
        "react-native-safe-area-context": "^1.0.0",
        "react-native-screens": "^2.7.0",
        "react-native-splash-screen": "^3.2.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/runtime": "^7.6.2",
        "@bam.tech/react-native-make": "^3.0.0",
        "@react-native-community/eslint-config": "^1.0.0",
        "@types/jest": "^24.0.24",
        "@types/react-native": "^0.62.0",
        "@types/react-test-renderer": "16.9.2",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "babel-jest": "^24.9.0",
        "eslint": "^6.5.1",
        "jest": "^24.9.0",
        "metro-react-native-babel-preset": "^0.58.0",
        "prettier": "^2.0.4",
        "react-test-renderer": "16.11.0",
        "typescript": "^3.8.3"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
